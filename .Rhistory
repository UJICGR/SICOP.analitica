#' consulta_contratistas_adjudicaciones_cantidad_anio
#'
#' @return consulta_contratistas_cantidad_anio_adjudicaciones
#' @importFrom sqldf sqldf
#' @export
#'
#' @examples
consulta_contratistas__adjudicaciones_cantidad_anio <-function(cantidad,anio){
if(is.null(pkg.globals$datos_sicop)){carga_datos()}
datos_sicop<-pkg.globals$datos_sicop
consulta<-paste0("select * from datos_sicop where ANO_ADJUDICADO=",anio," order by MONTO_ADJUDICADO_LINEA limit",as.numeric(cantidad))
return(sqldf(consulta))
}
